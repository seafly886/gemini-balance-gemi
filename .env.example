# 数据库配置
DATABASE_TYPE=sqlite
SQLITE_DATABASE=gemini_balance.db
# API Keys 配置
API_KEYS=["AIzaSyxxxxxxxxxxxxxxxxxxx","AIzaSyxxxxxxxxxxxxxxxxxxx"]

# 认证配置
ALLOWED_TOKENS=["sk-123456"]
AUTH_TOKEN=sk-123456
# Vertex AI 配置
VERTEX_API_KEYS=["AQ.Abxxxxxxxxxxxxxxxxxxx"]
VERTEX_EXPRESS_BASE_URL=https://aiplatform.googleapis.com/v1beta1/publishers/google
# 模型配置
TEST_MODEL=gemini-1.5-flash
THINKING_MODELS=["gemini-2.0-flash-thinking-exp"]
THINKING_BUDGET_MAP={"gemini-2.0-flash-thinking-exp": 4000}
IMAGE_MODELS=["gemini-2.0-flash-exp"]
SEARCH_MODELS=["gemini-2.0-flash-exp"]
FILTERED_MODELS=["gemini-1.0-pro-vision-latest", "gemini-pro-vision"]
# 功能开关
URL_CONTEXT_ENABLED=true
URL_CONTEXT_MODELS=["gemini-2.0-flash-exp"]
TOOLS_CODE_EXECUTION_ENABLED=false
SHOW_SEARCH_LINK=true
SHOW_THINKING_PROCESS=true
# 基础配置
BASE_URL=https://generativelanguage.googleapis.com/v1beta
MAX_FAILURES=5
MAX_RETRIES=3
CHECK_INTERVAL_HOURS=1
TIMEZONE=Asia/Shanghai
TIME_OUT=300
# 代理配置
PROXIES=[]
PROXIES_USE_CONSISTENCY_HASH_BY_API_KEY=true
# 图像生成配置
PAID_KEY=AIzaSyxxxxxxxxxxxxxxxxxxx
CREATE_IMAGE_MODEL=imagen-3.0-generate-002
UPLOAD_PROVIDER=smms
SMMS_SECRET_TOKEN=your_smms_token_here
# 流式输出优化配置
STREAM_OPTIMIZER_ENABLED=false
STREAM_MIN_DELAY=0.016
STREAM_MAX_DELAY=0.024
STREAM_SHORT_TEXT_THRESHOLD=10
STREAM_LONG_TEXT_THRESHOLD=50
STREAM_CHUNK_SIZE=5
# 日志配置
LOG_LEVEL=info
AUTO_DELETE_ERROR_LOGS_ENABLED=true
AUTO_DELETE_ERROR_LOGS_DAYS=7
AUTO_DELETE_REQUEST_LOGS_ENABLED=false
AUTO_DELETE_REQUEST_LOGS_DAYS=30

# 假流式配置
FAKE_STREAM_ENABLED=false
FAKE_STREAM_EMPTY_DATA_INTERVAL_SECONDS=5

# 安全设置
SAFETY_SETTINGS=[{"category": "HARM_CATEGORY_HARASSMENT", "threshold": "BLOCK_MEDIUM_AND_ABOVE"}, {"category": "HARM_CATEGORY_HATE_SPEECH", "threshold": "BLOCK_MEDIUM_AND_ABOVE"}, {"category": "HARM_CATEGORY_SEXUALLY_EXPLICIT", "threshold": "BLOCK_MEDIUM_AND_ABOVE"}, {"category": "HARM_CATEGORY_DANGEROUS_CONTENT", "threshold": "BLOCK_MEDIUM_AND_ABOVE"}]

# 智能路由配置
URL_NORMALIZATION_ENABLED=false

# TTS配置
TTS_MODEL=gemini-2.0-flash-exp
TTS_VOICE_NAME=Zephyr
TTS_SPEED=normal
# Files API 配置
FILES_CLEANUP_ENABLED=true
FILES_CLEANUP_INTERVAL_HOURS=1
FILES_USER_ISOLATION_ENABLED=true

# Key使用模式配置
KEY_USAGE_MODE=polling
KEY_USAGE_THRESHOLD=200